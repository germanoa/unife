/*
 * - Test 1 -
 * Testa inicializacao da lib.
 *
 * - Resultado Esperado:
 * Sistema iniciado com sucesso.
 */

#include <stdio.h>
#include <include/unucleo.h>

void *f1(void *arg)
{
	int i = 0, x = 0;

	for(i = 0; i < 1000; i++)
	{
		x += i;
		mproc_yield();
	}

    return NULL;
}

int main (int argc, char *argv[])
{
    int system;

    system = libsisop_init();    
    
    if ( system == 0 )
    {
    	printf("Sistema iniciado com sucesso.\n");
    }
    
    scheduler();

    return system;
}
